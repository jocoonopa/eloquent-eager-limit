{
    "name": "jocoonopa/eloquent-eager-limit",
    "description": "Laravel Eloquent eager loading with limit",
    "license": "MIT",
    "authors": [
        {
            "name": "jocoonopa",
            "email": "jocoonopa@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/database": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "Staudenmeir\\EloquentEagerLimit\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Staudenmeir\\EloquentEagerLimit\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
